{
  "manifest_version": 3,
  "name": "YYQ利器",
  "description": "for practice",
  "options_page": "options.html",
  "background": { "service_worker": "background.bundle.js" },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "logo.png"
  },
  "chrome_url_overrides": {
    "newtab": "tabs.html"
  },
  "icons": {
    "128": "logo.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.bundle.js"],
      "css": ["content.styles.css"]
    }
  ],
  "devtools_page": "devtools.html",
  "web_accessible_resources": [
    {
      "resources": ["content.styles.css", "logo.png", "logo.png"],
      "matches": []
    }
  ],
  "permissions": [
    "storage",
    "tabs",
    "alarms",
    "notifications",
    "cookies",
    "webRequest"
  ],
  "sandbox": {
    "pages": ["sandbox.html"]
  },
  "host_permissions": [
    "<all_urls>",
    "*://*/*"
  ],

  "commands": {
    "add-tabs": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": "add tabs"
    },
    "injectEdit": {
      "suggested_key": {
        "default": "Ctrl+Shift+1",
        "mac": "Command+Shift+1"
      },
      "description": "让网页可编辑，复制"
    },
    "showCookies": {
      "suggested_key": {
        "default": "Ctrl+Shift+2",
        "mac": "Command+Shift+2"
      },
      "description": "showCookies"
    }
  }
}
