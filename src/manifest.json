{
  "manifest_version": 3,
  "name": "tab管理",
  "description": "tab管理，管理好你的tab页面",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["icon-128.png", "icon-34.png"],
      "matches": []
    }
  ],
  "permissions": ["tabs"]
}
